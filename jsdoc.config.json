{
  "source": {
    "include": ["./index.js"],
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc", "closure"]
  },
  "plugins": [
    "plugins/markdown"
   ],
   "opts": {
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "./docs/",
    "readme": "README.md",
    "recurse": true,
    "verbose": true
   },
   "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  }
} 